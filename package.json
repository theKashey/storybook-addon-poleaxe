{
  "name": "storybook-addon-poleaxe",
  "version": "0.1.1",
  "description": "If there is storybook-axe, why not to have a poleaxe?",
  "main": "dist/es5/index.js",
  "module": "dist/es2015/index.js",
  "module:es2019": "dist/es2019/index.js",
  "types": "dist/es5/index.d.ts",
  "storybook": {
    "displayName": "Storybook poleaxe",
    "icon": "https://raw.githubusercontent.com/theKashey/storybook-include/main/assets/logo.png"
  },
  "scripts": {
    "test": "jest",
    "dev": "start-storybook --config-dir example/.storybook --no-manager-cache",
    "test:ci": "jest --runInBand --coverage",
    "build": "lib-builder build",
    "release": "yarn build && yarn test",
    "lint": "lib-builder lint",
    "format": "lib-builder format",
    "update": "lib-builder update",
    "prepublish": "yarn build && yarn changelog",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog:rewrite": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "author": "theKashey <thekashey@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@size-limit/preset-small-lib": "^2.1.6",
    "@storybook/cli": "^6.3.12",
    "@storybook/core": "^6.1.21",
    "@storybook/react": "^6.3.12",
    "@theuiteam/lib-builder": "^0.1.2",
    "@types/node": "^16.11.9",
    "eslint-plugin-prettier": "3.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "engines": {
    "node": ">=10"
  },
  "peerDependencies": {
    "@storybook/addons": "^5.0.0 || ^6.0.0",
    "@storybook/api": "^5.0.0 || ^6.0.0",
    "@storybook/components": "^5.0.0 || ^6.0.0",
    "@storybook/core-events": "^5.0.0 || ^6.0.0",
    "@types/react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "dependencies": {
    "tslib": "^2.0.0",
    "yoxel": "^0.1.0"
  },
  "files": [
    "dist",
    "preset.js",
    "preset-entry"
  ],
  "keywords": [
    "addon",
    "storybook",
    "addons",
    "storybook-addons",
    "a11y"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theKashey/storybook-addon-poleaxe.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  }
}
